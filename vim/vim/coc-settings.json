{
    "http": {
        "proxy": "127.0.0.1:12759"
    },
    "javascript": {
        "suggestionActions": {
            "enabled": false
        }
    },
    "languageserver": {
        "bash": {
          "command": "bash-language-server",
          "args": ["start"],
          "filetypes": ["sh"],
          "ignoredRootPaths": ["~"]
        },
        "efm": {
            "command": "efm-langserver",
            "args": ["-c", "~/.vim/config/plugin/coc/efm-langserver/config.yaml"],
            "filetypes": ["eruby", "markdown"]
        }
    }
}

